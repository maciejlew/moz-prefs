{
  "name": "lionnet/moz-prefs",
  "type": "library",
  "description": "MOZ prefs utilities",
  "keywords": ["browsers"],
  "homepage": "https://github.com/maciejlew/moz-prefs",
  "license": "MIT",
  "require": {
    "php": "^7.1 || ^8.0",
    "symfony/yaml": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6 || ^8",
    "mikey179/vfsstream": "^1.6",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "LionNet\\MozPrefs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LionNet\\MozPrefs\\Tests\\": "tests/"
    }
  }
}
